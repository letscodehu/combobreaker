{
  "name": "letscodehu/combobreaker-dummy",
  "description": "Letscode.hu Combobreaker dummy",
  "type": "project",
  "homepage": "https://www.letscode.hu",
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "phpmd/phpmd": "@stable"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
